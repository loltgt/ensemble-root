{
  "name": "ensemble-root",
  "version": "0.0.1",
  "description": "loltgt ensemble root source tree",
  "main": "index.js",
  "scripts": {
    "build": "BASEPATH=**/ ./node_modules/.bin/gulp",
    "build-js": "BASEPATH=**/ ./node_modules/.bin/gulp build_js",
    "build-css": "BASEPATH=**/ ./node_modules/.bin/gulp build_css",
    "build-compat": "BASEPATH=**/ ./node_modules/.bin/gulp compat",
    "build-compat-js": "BASEPATH=**/ ./node_modules/.bin/gulp compat_js",
    "build-compat-css": "BASEPATH=**/ ./node_modules/.bin/gulp compat_css",
    "watch": "BASEPATH=**/ ./node_modules/.bin/gulp watch",
    "demo": "./node_modules/.bin/gulp demo",
    "clone-deps-all": "cd ./ensemble; rm -R node_modules; cp ../node_modules node_modules; cd ../ensemble-stack-d1; rm -R node_modules; cp ../node_modules node_modules; cd ../ensemble-modal; rm -R node_modules; cp ../node_modules node_modules; cd ../ensemble-lightbox; rm -R node_modules; cp ../node_modules node_modules; cd ../ensemble-social-share; rm -R node_modules; cp ../node_modules node_modules",
    "git-push-all": "cd ./ensemble; git push origin master; cd ../ensemble-stack-d1; git push origin master; cd ../ensemble-modal; git push origin master; cd ../ensemble-lightbox; git push origin master; cd ../ensemble-social-share; git push origin master",
    "delete-sourcemaps-all": "find . -name *ensemble*-*.map -type f -delete",
    "docs": "rm -R ./out; ./node_modules/.bin/jsdoc ./ensemble/*.js -r; ./node_modules/.bin/jsdoc ./ensemble-stack-d1/*.js -r; ./node_modules/.bin/jsdoc ./ensemble-modal/src/js/*.js -r; ./node_modules/.bin/jsdoc ./ensemble-lightbox/src/js/*.js -r; ./node_modules/.bin/jsdoc ./ensemble-social-share/src/js/*.js -r"
  },
  "repository": "git+https://github.com/loltgt/ensemble-root.git",
  "author": "Leonardo Laureti",
  "license": "MIT",
  "homepage": "https://github.com/loltgt/ensemble-root#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/plugin-transform-modules-umd": "^7.13.0",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/preset-env": "^7.13.10",
    "@rbnlffl/gulp-rollup": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tap": "^2.0.0",
    "gulp-terser": "^2.0.1",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^7.0.0",
    "node-sass": "^5.0.0",
    "regenerator-runtime": "^0.13.7"
  }
}
